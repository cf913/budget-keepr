{
  "expo": {
    "name": "budget-keepr",
    "slug": "budget-keepr",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "backgroundColor": "#000",
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.budgetkeepr",
      "buildNumber": "10"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "expo-font",
      [
        "@sentry/react-native/expo",
        {
          "organization": "cf913",
          "project": "react-native",
          "url": "https://sentry.io"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "160e0efd-d9b3-4f6e-b454-6a41cf0e32fd"
      }
    },
    "owner": "chad_fallotin",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/160e0efd-d9b3-4f6e-b454-6a41cf0e32fd"
    }
  }
}
