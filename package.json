{
  "name": "budget-keepr",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "eas:b:preview": "eas build --profile preview --platform ios --local",
    "eas:b:dev-client-ios": "eas build --profile development-simulator --platform ios --local",
    "eas:b:dev-client": "eas build --profile development-simulator --local",
    "eas:u:preview": "eas update --branch preview --message",
    "us": "node node_modules/@sentry/react-native/scripts/expo-upload-sourcemaps dist/",
    "android": "expo start --android",
    "tmux:start": "npm run ios -c",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:restart": "npm run supabase:stop && npm run supabase:start",
    "supabase:reset": "npx supabase db reset",
    "supabase:link": "npx supabase link",
    "supabase:generate-types": "npx supabase gen types typescript --local --schema public > types_db.ts",
    "supabase:generate-migration": "npx supabase db diff | npx supabase migration new",
    "supabase:generate-seed": "npx supabase db dump --data-only -f supabase/seed.sql",
    "supabase:generate-roles": "npx supabase db dump --role-only -f supabase/roles.sql",
    "supabase:push": "npx supabase db push",
    "supabase:pull": "npx supabase db pull"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@backpackapp-io/react-native-toast": "^0.11.0",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-picker/picker": "2.7.5",
    "@react-native-segmented-control/segmented-control": "2.5.2",
    "@react-navigation/elements": "^1.3.30",
    "@react-navigation/native": "^6.0.2",
    "@sentry/cli": "^2.32.1",
    "@sentry/react-native": "~5.22.0",
    "@supabase/supabase-js": "^2.43.4",
    "@tanstack/react-query": "^5.40.0",
    "dayjs": "^1.11.11",
    "expo": "~51.0.17",
    "expo-apple-authentication": "~6.4.1",
    "expo-application": "~5.9.1",
    "expo-blur": "~13.0.2",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.19",
    "expo-font": "~12.0.7",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.17",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.18",
    "expo-web-browser": "~13.0.3",
    "fuse.js": "^7.0.0",
    "promise": "^8.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-gifted-charts": "^1.4.10",
    "react-native-pager-view": "6.3.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.10",
    "reanimated-color-picker": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tanstack/eslint-plugin-query": "^5.35.6",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "eslint-config-expo": "^7.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
